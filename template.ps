%!PS

/in {72 mul} def
/h 2 3 4 div add in def
/w 1 3 16 div add in def
/fontsize 17 def
/sep 0.5 in def

/radius 10 def

% set to 1 for final print
/x_positions [0 3] def

% ------ Procedure for label ------
/label
{
    /text_arr exch def
    /y_pos exch def
    /x_pos exch def

    /x_off x_pos w sep add mul def
    /y_off y_pos h sep add mul def

    x_off y_off translate

    0 1 1
    {
        /n exch def
        /x x_positions n get def

        n 1.5 div setgray

        newpath
        x radius moveto

        x h radius sub lineto
        % Upper-Left Corner
        radius
        h radius sub
        radius x sub 180 90
        arcn

        w radius sub h x sub lineto
        % Upper-Right Corner
        w radius sub % x
        h radius sub % y
        radius x sub
        90 0 arcn

        w x sub h radius sub lineto
        % Bottom-Right Corner
        w radius sub
        radius
        radius x sub
        0 270 arcn

        radius x sub x lineto
        radius
        radius
        radius x sub
        270 180 arcn
        closepath
        stroke
    } for

    /middle_y h 2 div def
    /arr_len text_arr length def
    /line_height fontsize 1.3 mul def
    /font_off line_height fontsize sub 2 div def

    0 setgray
    1 1 arr_len
    {
        /i exch def
        /text text_arr i 1 sub get def
        /y_pos h 2 div line_height arr_len 2 div i sub mul add font_off add def
        /str_w text stringwidth pop def
        /x_pos w str_w sub 2 div def
        x_pos y_pos moveto
        text show
    } for

    -1 x_off mul -1 y_off mul translate
} def

/Helvetica-Narrow-Bold findfont fontsize scalefont setfont

newpath
%0 in 0.5 in moveto 8.5 in 0.5 in lineto
%0.5 in 0 moveto 0.5 in 11 in lineto
%8 in 0 moveto 8 in 11 in lineto
% 0 10.5 in moveto 8.5  in 10.5 in lineto
%0.75 setgray
%stroke

% horizontal cut lines
0.75 setgray
0 1 1
{
    /i exch def
    newpath
    /y 1 in 6 sub i h sep add mul add def
    0 y moveto
    8.5 in 0 rlineto
    stroke
    0 y h add 6 2 mul add moveto
    8.5 in 0 rlineto
    stroke
} for

0 setgray

1 in 1 in translate

%0 0 [(Juniper)(Berries)] label
%1 0 [(Celery)(Salt)] label
%2 0 [(Bell's)(Seasoning)] label
%3 0 [(Garlic)(Powder)] label
0 0 [(Paprika)(Hungarian)(Half-Sharp)] label
1 0 [(Chili)(Powder)] label
2 0 [(Cayenne)(Pepper)] label
3 0 [(Onion)(Powder)] label
showpage

